      <h1>#전체게시판</h1>
      <br>
      <br>
      
      <table class="table table-hover table-striped text-center" style="border: 1px solid;">
     
          <thead>
            <tr>
              <th>번호</th>
              <th>제목</th>
              <th>글쓴이</th>
              <th>조회수</th>
              <!--<th>댓글수</th>-->
              <th>등록 일자</th>

            </tr>
          </thead>
          
          <% if(locals.rows){ %>
            <% for(var i = 0;i<rows.length;i++) { %>
            <tbody>
              <tr onClick="location.href='/board/?number=<%= rows[i].number%>'">
                <td><%= rows[i].number%></td>
                <td><%= rows[i].title%></td>
                <td><%= rows[i].writer%></td>
                <td><%= rows[i].view_point%></td>
                <td><%= rows[i].regislation_date%></td>
              </tr>
          </tbody>
          <% } %>
          <% } %>   
         
      </table>
    
